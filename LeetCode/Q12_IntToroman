#include <bits/stdc++.h>
#include <string>
using namespace std;
class Solution {
public:
    char values(int num) {
        if (num == 1)
            return 'I';
        if (num == 5)
            return 'V';
        if (num == 10)
            return 'X';
        if (num == 50)
            return 'L';
        if (num == 100)
            return 'C';
        if (num == 500)
            return 'D';
        if (num == 1000)
            return 'M';
        return 0;
    };

    string intToRoman(int num) {
        
    }
};

int main(){
    Solution sol;
    int num =3749;
    cout<<sol.intToRoman(num);

    return 0;
}